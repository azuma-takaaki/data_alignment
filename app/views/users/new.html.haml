%h1 Users#new
%p Find me in app/views/users/new.html.haml

= form_for @user do |f|
  = f.label :name
  = f.text_field :name
  = f.label :login_id
  = f.text_field :login_id
  = f.label :password
  = f.password_field :password
  = f.label :password_confirmation
  = f.password_field :password_confirmation
  = f.submit

